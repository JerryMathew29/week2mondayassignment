   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Ask user if they want add, multiply, subtract, or divide\n",
    "from IPython.display import clear_output\n",
    "\n",
    "i = 0\n",
    "while i < 3:\n",
    "    answer = input(\"What would you like to do (add, multiply, subtract, or divide)? Type 'quit' to end the program. \")\n",
    "    clear_output()\n",
    "    \n",
    "    if answer == 'quit':\n",
    "        break\n",
    "        \n",
    "    try:\n",
    "        num1 = float(input(\"What's the first number? \"))\n",
    "        num2 = float(input(\"What's the second number? \"))\n",
    "        \n",
    "        if answer == 'add':\n",
    "            print(f\"{num1} + {num2} = {num1 + num2}\")\n",
    "        elif answer == 'subtract':\n",
    "            print(f\"{num1} - {num2} = {num1 - num2}\")\n",
    "        elif answer == 'multiply':\n",
    "            print(f\"{num1} * {num2} = {num1 * num2}\")\n",
    "        elif answer == 'divide':\n",
    "            print(f\"{num1} / {num2} = {num1 / num2}\")\n",
    "        else:\n",
    "            print(\"That operation was invalid. Try again. \")\n",
    "    except ValueError:\n",
    "        print(\"That was an invalid character, try again...\")"
   ]
  },
  {
   "cell_type": "markdown",
